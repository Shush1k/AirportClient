<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ToolBar?>
<?import javafx.scene.layout.BorderPane?>

<BorderPane prefHeight="700.0" prefWidth="1000.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="program.controllers.MainLayoutController">
<top>
    <ToolBar prefHeight="40.0" prefWidth="269.0" BorderPane.alignment="CENTER">
        <items>
            <Button mnemonicParsing="false" onAction="#openArrivalBoard" prefHeight="27.0" prefWidth="73.0" text="Прилеты" />
            <Button mnemonicParsing="false" onAction="#openDepartureBoard" prefHeight="27.0" prefWidth="64.0" text="Вылеты" />
            <Button mnemonicParsing="false" onAction="#openAirlineInfo" prefHeight="27.0" prefWidth="107.0" text="Авиакомпании" />
            <Button mnemonicParsing="false" onAction="#openEditPerson" prefHeight="27.0" prefWidth="120.0" text="Личный Кабинет" />
        </items>
    </ToolBar>
</top>
</BorderPane>
